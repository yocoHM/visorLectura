<% if logged_in? %>
	
	
	<div class="dropdown">
		<%= link_to "#", class: "btn btn-default btn-warning dropdown-toggle", "data-toggle" => "dropdown" do %>
			<%= usuario_actual.nombre %> <b class="caret"></b>
		<% end %>
		<ul class="dropdown-menu">
			<li><%= link_to "Logout", logout_path(usuario_actual) %></li>
		</ul>
	</div>
	
	
<% end %>